public static void main(String[] args) {
    int a = slow_add(5, 4);
    int b = slow_add(5, 5);
    ensure(a == b);
}

public static int slow_add(int m, int n) {
    boolean keep_going = true;
    int step = 1;
    int i = 0;
    int sum = m;

    while (keep_going) {
        sum += step;
        i += 1;

        if (i >= n) {
            keep_going = false;
        }
    }

    return sum;
}
